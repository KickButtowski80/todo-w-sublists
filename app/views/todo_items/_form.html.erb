<%#= form_for([@todo_list, @todo_list.@todo_list.todo_items.builds.build]) do |form| %>
<%= form_with model: [@todo_list, @todo_list.todo_items.build], local: true do |form| %>
   <% if @todo_list.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@todo_list.errors.count, "error") %> prohibited
        this article from being saved:
      </h2>
      <ul>
        <% @todo_list.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>



  <div class="field">
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div class="field">
    <%= form.label :completed %>
    <%= form.check_box :completed %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.collection_radio_buttons :status, [[:low, 'Low'] ,[:medium, 'Medium'],[:high, 'High'],[:urgent, 'Urgent']], :first, :last %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
